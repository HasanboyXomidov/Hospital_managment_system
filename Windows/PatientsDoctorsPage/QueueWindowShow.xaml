<Window x:Class="Hospital_managment_system.Windows.PatientsDoctorsPage.QueueWindowShow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hospital_managment_system.Windows.PatientsDoctorsPage"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="QueueWindowShow" Height="550" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="500"/>
            
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0"
               Content="7"
               Foreground="{DynamicResource BlueButton}"
               FontSize="200"
               Padding="0"            
               x:Name="lblqueue"
               HorizontalAlignment="Center"
               VerticalAlignment="Center"
               Margin="0">
        </Label>
        
    </Grid>
</Window>
